{
  "fmt": {
  "useTabs": true,
  "lineWidth": 80,
  "indentWidth": 4,
  "semiColons": true,
  "singleQuote": true,
  "proseWrap": "preserve",
  "include": ["src/"],
  "exclude": ["src/testdata/", "src/fixtures/**/*.ts"]
  },
  "tasks": {
    "dev": "deno run --env-file --allow-read --allow-env --allow-net --watch main.ts",
    "lint": "deno lint"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "cors": "npm:cors@^2.8.5",
    "express": "npm:express@^4.21.1",
    "socket.io": "npm:socket.io@^4.8.1"
  }
}